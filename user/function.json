{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [ "get", "post", "delete", "put" ],
      "route": "{action}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}